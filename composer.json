{
  "name": "alizharb/filament-game-icons",
  "description": "Game Icons Pack ready for Filament 4",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "filament",
    "icons",
    "game-icon",
    "library",
    "iconset"
  ],
  "authors": [
    {
      "name": "Ali Harb",
      "email": "harbzali@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "codeat3/blade-game-icons": "^2.3",
    "filament/filament": "^4.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "AlizHarb\\FilamentGameIcons\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlizHarb\\FilamentGameIcons\\GameIconsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
